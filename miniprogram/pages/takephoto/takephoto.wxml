<!-- 拍照功能 -->
<!-- <camera style="width:100%；height=300px"></camera> -->
<camera mode="normal" device-position="back" flash="auto" binderror="error" style="width: 100%;height: 100%;position: absolute;">
    <cover-view class="controls flex justify-center" style=" padding-top: 160rpx">
        <cover-image class="img" src="https://s3.ax1x.com/2020/11/16/DEeVbt.png" style="width:90%" />
    </cover-view>
    <cover-image bindtap="takePhoto" class="photo padding-bottom-xl" src="https://s3.ax1x.com/2020/11/13/D9KR3R.png" mode="widthFix" ></cover-image>
</camera>
